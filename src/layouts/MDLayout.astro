---
import Layout from "@layouts/Layout.astro";

interface Props {
	frontmatter: {
		title?: string;
		description?: string;
		showFooterGoBack?: boolean;
		backUrl?: string;
	};
}
const { frontmatter } = Astro.props;
---

<Layout {...frontmatter}>
	<article class="prose slide-enter-content m-auto color-dark/75 dark:color-light/75">
		<slot />
	</article>
</Layout>
